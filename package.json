	"scripts": {
		"build:microservice": "rimraf dist/apps & nest build history-service & nest build event-service & nest build sync-service",
		"build": "nest build dni-service",
		"format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
		"start": "nest start",
		"start:dev:all": "concurrently \"npm run start:dev\" \"npm run start:dev event-service\" \"npm run start:dev history-service\" \"npm run start:dev sync-service\"",
		"copy-proto": "cp -r ./libs/common/src/grpc/protos ./dist",
		"start:dev": "NODE_ENV=development nest start --watch",
		"start:dev:webpack": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
		"start:debug": "NODE_ENV=development nest start --debug --watch",
		"start:prod": "NODE_ENV=production node dist/apps/nestjs-rabbitmq/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./apps/nestjs-rabbitmq/test/jest-e2e.json"
	},
